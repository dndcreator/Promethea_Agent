# Global LLM API (shared by all users)
API__API_KEY=your_api_key_here
API__BASE_URL=https://openrouter.ai/api/v1
API__MODEL=nvidia/nemotron-3-nano-30b-a3b:free

# Auth / Gateway
AUTH__SECRET_KEY=change_me_to_a_long_random_string
# Optional: if set, gateway websocket clients must send this token in connect payload
GATEWAY_AUTH_TOKEN=

# Optional: Memory subsystem
MEMORY__ENABLED=false
MEMORY__NEO4J__ENABLED=false
MEMORY__NEO4J__URI=bolt://127.0.0.1:7687
MEMORY__NEO4J__USERNAME=neo4j
MEMORY__NEO4J__PASSWORD=password
MEMORY__NEO4J__DATABASE=neo4j

# System
SYSTEM__LOG_LEVEL=INFO
SYSTEM__DEBUG=false
